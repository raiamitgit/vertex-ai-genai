<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{YYYY-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <!-- Set the root logger level. Change to DEBUG for more verbose output -->
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>

    <!-- You can set levels for specific packages -->
    <!-- <logger name="com.example.vertexaisearch" level="DEBUG"/> -->
    <!-- <logger name="com.google.api.gax" level="INFO"/> -->

</configuration>